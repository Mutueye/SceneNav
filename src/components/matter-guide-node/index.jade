if nodeType == 'selected'
	.matter-head.sel
		.matter-head-number!= headNumber
		.matter-head-text!= selectedName
		.matter-head-tail
		.dotted-vline
		.matter-head-number-arrow
		a.btn-reselect(href = '#!/matters/' + currentUrl) 重选
if nodeType == 'end'
	if dataType == 'list'
		.matter-head
			.matter-head-number!= headNumber
			.matter-head-text 请从以下分类中选择：
			.matter-head-tail
			.dotted-vline
			.matter-head-number-arrow
		.matter-infobox
			.matter-infobox-corner
			.matter-infobox-content
				.row
					for btn, i in btns
						.col-1of4.col-mobile-1of1
							.col-content
								if(btn['Type'] == 'list' || btn['Type'] == 'info')
									a.btn-select-matter(href = '#!/matters/' + currentUrl + '_' + btn['id'])!= btn['Name']
								if(btn['Type'] == 'link')
									a.btn-select-matter(href = btn['ChildData'] target = '_blank')!= btn['Name']
	if dataType == 'info'
		.matter-head
			.matter-head-text-2(id = 'title_'+currentUrl)
			.matter-head-tail-2
			.dotted-vline
		.matter-infobox(id = 'info_'+currentUrl)

		